{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "projections": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "date":{
            "type": "object",
            "properties": {
              "jour": {"type": "integer"},
              "mois": {"type": "integer"},
              "annee": {"type": "integer"},
              "heure": {"type": "integer"},
              "minute": {"type": "integer"}
            }
          },
          "film": {
            "type": "object",
            "properties": {
              "titre": {"type": "string"},
              "acteurs": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "nom_naissance": {"type": "string"},
                    "role": {"type": "string"}
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}